<% provide(:titulo, "Sign up") %>

<%= render "shared/error_messages" %>

<%= form_for @usuario do |f| %>
    <%= f.label :name, "Nombre" %> <br>
    <%= f.text_field :name, class: 'form-control'  %> <br>

    <%= f.label :email, "Correo electronico" %> <br>
    <%= f.text_field :email, class: 'form-control'  %> <br>

    <%= f.label :password, "Password" %> <br>
    <%= f.password_field :password, class: 'form-control'  %> <br>

    <%= f.label :password_confirmation, "Repetir password" %> <br>
    <%= f.password_field :password_confirmation, class: 'form-control'  %> <br>

    <%= f.submit "Crear cuenta", :class => "btn btn-primary" %> <br>

<% end %>

